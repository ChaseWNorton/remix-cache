{
  "name": "remix-cache",
  "version": "0.1.0",
  "description": "A comprehensive, type-safe caching library built specifically for Remix applications with distributed caching, intelligent invalidation, and seamless client-server integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "remix",
    "cache",
    "caching",
    "redis",
    "distributed-cache",
    "server-side-cache",
    "invalidation",
    "typescript"
  ],
  "author": "Chase W. Norton",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "remix-utils": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@remix-run/react": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "remix-utils": {
      "optional": true
    }
  },
  "devDependencies": {
    "@remix-run/node": "^2.17.2",
    "@remix-run/react": "^2.17.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "happy-dom": "^20.0.10",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "remix-utils": "^7.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "ioredis": "^5.8.2",
    "lru-cache": "^11.2.2",
    "superjson": "^2.2.5"
  }
}
